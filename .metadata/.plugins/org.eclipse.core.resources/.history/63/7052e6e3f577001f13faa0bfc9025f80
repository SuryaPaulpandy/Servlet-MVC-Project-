package demo;
import java.util.*;

public class Demo1 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the string");
		String s1 = sc.nextLine();
//		String s2 = " ";
		for(char x : s1.toCharArray()) {
			String s2 = x+" ";
//			System.out.println(x);
		}
		System.out.println(s2);
	}

}
